<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="@dimen/qb_px_30"
    android:layout_marginRight="@dimen/qb_px_30"
    android:orientation="vertical"
    android:gravity="center"
    android:background="@color/bgcolor_white">

    <EditText
        android:id="@+id/ed_dialog_edit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="测试"/>

    <Button
        android:id="@+id/btn_dialog_edit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="确定"/>
    <Button
        android:id="@+id/btn2_dialog_edit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="取消"/>

</LinearLayout>
