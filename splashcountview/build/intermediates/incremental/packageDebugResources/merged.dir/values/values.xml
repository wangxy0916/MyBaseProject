<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CountDownView">
        <!--view半径-->
        <attr format="dimension" name="cd_circle_radius"/>
        <!--画笔宽度-->
        <attr format="dimension" name="cd_arc_width"/>
        <!--画笔颜色-->
        <attr format="color" name="cd_arc_color"/>
        <!--背景颜色-->
        <attr format="color" name="cd_bg_color"/>
        <!--字体颜色-->
        <attr format="color" name="cd_text_color"/>
        <!--字体尺寸-->
        <attr format="dimension" name="cd_text_size"/>
        <!--动画执行时长-->
        <attr format="integer" name="cd_animator_time"/>
        <!--时间单位-->
        <attr format="string" name="cd_animator_time_unit"/>
        <!--动画进退方式-->
        <attr format="enum" name="cd_retreat_type">
            <!--外层的圆弧逐渐变长-->
            <enum name="forward" value="1"/>
            <!--外层的圆弧逐渐减短-->
            <enum name="back" value="2"/>
        </attr>
        <!--加载进度的开始位置-->
        <attr format="enum" name="cd_location">
            <enum name="left" value="1"/>
            <enum name="top" value="2"/>
            <enum name="right" value="3"/>
            <enum name="bottom" value="4"/>
        </attr>
    </declare-styleable>
    <string name="app_name">counttimeview</string>
</resources>